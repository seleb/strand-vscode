{
	"private": true,
	"name": "strand-vscode",
	"displayName": "strand VSCode",
	"description": "support for strand files",
	"icon": "strandIcon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/seleb/strand-vscode.git"
	},
	"author": "Sean S. LeBlanc <sean.s.leblanc@gmail.com>",
	"version": "0.0.4",
	"publisher": "seansleblanc",
	"engines": {
		"vscode": "^1.21.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/extension",
	"activationEvents": [
		"onLanguage:strand"
	],
	"contributes": {
		"languages": [
			{
				"id": "strand",
				"aliases": [
					"strand",
					"strand"
				],
				"extensions": [
					".strand",
					".str"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "strand",
				"scopeName": "source.strand",
				"path": "./syntaxes/strand.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.10.3",
		"tslint": "^5.12.1",
		"typescript": "^2.9.2",
		"vscode": "^1.1.29"
	}
}
