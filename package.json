{
	"name": "strand-vscode",
	"displayName": "strand VSCode",
	"description": "support for strand files",
	"repository": "seleb/strand-vscode",
	"author": "Sean S. LeBlanc <sean.s.leblanc@gmail.com>",
	"version": "0.0.1",
	"publisher": "seansleblanc",
	"engines": {
		"vscode": "^1.21.0"
	},
	"categories": [
		"Languages"
	],
	"main": "./out/extension",
	"activationEvents": [
		"onLanguage:strand"
	],
	"contributes": {
		"languages": [
			{
				"id": "strand",
				"aliases": [
					"strand",
					"strand"
				],
				"extensions": [
					".strand",
					".str"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "strand",
				"scopeName": "source.strand",
				"path": "./syntaxes/strand.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"tslint": "^5.8.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	}
}
